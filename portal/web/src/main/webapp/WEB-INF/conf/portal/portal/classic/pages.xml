      <id>1</id>
      <portlet>
        <name>p1</name>
        <access-permission>Everyone</access-permission>
        <application-ref>samples</application-ref>
        <portlet-ref>ParametersPortlet</portlet-ref>
      </portlet>
      <portlet>
        <name>p2</name>
        <access-permission>Everyone</access-permission>
        <application-ref>samples</application-ref>
